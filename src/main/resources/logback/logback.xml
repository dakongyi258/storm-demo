<?xml version="1.0" encoding="UTF-8"?>
<configuration>
    <appender name="STDOUT" class="ch.qos.logback.core.ConsoleAppender">
        <encoder>
            <pattern>%d{HH:mm:ss} %5p [%10.10t] %-30.30logger{29} %m%n</pattern>
        </encoder>
    </appender>

    <root level="WARN">
        <appender-ref ref="STDOUT"/>
    </root>
    <logger level="INFO" name="com.fn.stormdemo"/>
    <logger level="INFO" name="p6spy"/>

</configuration>





